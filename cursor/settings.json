{
    "window.commandCenter": true,
    "window.openFoldersInNewWindow": "on",
    "window.restoreWindows": "preserve",
    "workbench.colorTheme": "JetBrains New Dark",
    "workbench.list.openMode": "doubleClick",
    "workbench.list.smoothScrolling": true,
    "editor.fontFamily": "Fira Code, Arial",
    "editor.fontWeight": "400",
    "editor.glyphMargin": false,
    "workbench.fontAliasing": "antialiased",
    "workbench.iconTheme": "vscode-icons",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 20,
    "editor.letterSpacing": -0.3,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "none",
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "cursor.cpp.disabledLanguages": [
        "plaintext"
    ],
    "search.exclude": {
        "**/node_modules": true,
        "**/.jekyll-cache": true,
        "**/_site": true
    },
    "workbench.colorCustomizations": {
        "list.inactiveSelectionBackground": "#43454b",
        "list.activeSelectionBackground": "#2e436e",
        "tab.activeBackground": "#1e1f22",
        "tab.activeForeground": "#dfe1e5",
        "tab.inactiveBackground": "#1e1f22",
        "tab.border": "#1e1f22",
        "tab.activeBorder": "#0078ff",
        "tab.activeBorderTop": "#1e1f22",
        "editor.lineHighlightBackground": "#26282d",
        "editor.lineHighlightBorder": "#26282d",
        "editor.selectionBackground": "#134388",
        "sideBar.background": "#2b2d30",
        "sideBar.foreground": "#dfe1e5",
        "statusBar.border": "#1a1a1a",
    },
    "git.openRepositoryInParentFolders": "never",
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "update.releaseTrack": "prerelease",
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "php.validate.executablePath": "/Users/albertoroura/.php/php",
    "terminal.integrated.allowedLinkSchemes": [
        "file",
        "http",
        "https",
        "mailto",
        "vscode",
        "vscode-insiders",
        "docker-desktop"
    ],
    "cursor.general.disableHttp2": true,
    "cursor.composer.shouldAllowCustomModes": true,
    "editor.largeFileOptimizations": false,
    "vsicons.dontShowNewVersionMessage": true,
    "cursor.composer.shouldChimeAfterChatFinishes": true
}
